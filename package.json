{
  "name": "wise-crack",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "transpile": "rm -rf build/ && npx tsc",
    "build": "npm run transpile",
    "start": "NODE_ENV=production node ./build/index.js",
    "dev": "NODE_ENV=development npx ts-node-dev ./src/index.ts",
    "lint": "eslint --ignore-path .eslintignore './src/**/*.{ts,js}'",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(js|ts|json)\""
  },
  "dependencies": {
    "@typegoose/typegoose": "^10.0.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "~4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.0.1",
    "http-errors": "~2.0.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.8.0",
    "morgan": "~1.10.0",
    "sanitize": "^2.1.2",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/helmet": "^4.0.0",
    "@types/http-errors": "^2.0.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.15",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
